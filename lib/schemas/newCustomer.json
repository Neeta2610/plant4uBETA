{
    "$id": "newCustomer",
    "type": "object",
    "properties": {
        "email": {
            "type": "string",
            "format": "emailAddress"
        },
        "deliveryaddress": {
            "type": "array"
        },
        "phone": {
            "type": "string",
            "isNotEmpty": true
        },
        "password": {
            "type": "string",
            "isNotEmpty": true
        },
        "newsletter": {
            "type": "boolean"
        }
    },
    "required": [
        "email",
        "phone",
        "password"
    ]
}